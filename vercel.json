{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.8" }
        },
        {
            "src": "autojwt/*.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/autojwt/(.*)",
            "dest": "/autojwt/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}

